@model FrontendDTO.EventRealLife

<div class="form-group">
    <label asp-for="Service.Name" class="control-label"></label>
    <input asp-for="Service.Name" class="form-control" />
    <span asp-validation-for="Service.Name" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="RestaurantId" class="control-label"></label>
    <select asp-for="RestaurantId" class ="form-control" asp-items="ViewBag.RestaurantId"></select>
</div>
<div class="form-group">
    <label asp-for="AsDelivery" class="control-label"></label>
    <input asp-for="AsDelivery" class="checkbox" type="checkbox" />
</div>
<div class="form-group">
    <label asp-for="UserLocationId" class="control-label"></label>
    <select asp-for="UserLocationId" class ="form-control" asp-items="ViewBag.UserLocationId"></select>
</div>
<div class="form-group">
    <label asp-for="PaymentMethod" class="control-label"></label>
    <select asp-for="PaymentMethod" class ="form-control" asp-items="ViewBag.PaymentMethod"></select>
    <span asp-validation-for="PaymentMethod" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="FirstName" class="control-label"></label>
    <input asp-for="FirstName" class="form-control" />
    <span asp-validation-for="FirstName" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="LastName" class="control-label"></label>
    <input asp-for="LastName" class="form-control" />
    <span asp-validation-for="LastName" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Phone" class="control-label"></label>
    <input asp-for="Phone" class="form-control" />
    <span asp-validation-for="Phone" class="text-danger"></span>
</div>